<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NavigationView">
        <attr name="navigation_has_back_button" format="boolean" />
        <attr name="navigation_background_color" format="reference" />
        <attr name="navigation_icon_color" format="reference" />
        <attr name="navigation_title_color" format="reference" />
        <attr name="navigation_title" format="string" />
        <attr name="navigation_first_option_icon" format="reference" />
        <attr name="navigation_first_option_accessibility" format="string"/>
        <attr name="navigation_second_option_icon" format="reference" />
        <attr name="navigation_second_option_accessibility" format="string"/>
        <attr name="navigation_text_link_label" format="string"/>
        <attr name="navigation_style">
            <enum name="dark_mode" value="0" />
            <enum name="azul" value="1" />
            <enum name="vertical_health" value="2" />
            <enum name="vertical_banking" value="3" />
            <enum name="holding" value="4" />
            <enum name="insurance" value="5" />
            <enum name="white" value="6" />
            <enum name="gray" value="7" />
            <enum name="off_white" value="8" />
        </attr>
    </declare-styleable>

</resources>